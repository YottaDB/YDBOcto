drop table "ABCD"; -- Error: Unknown table 'ABCD'
drop table "abcd";

create table abcd (id integer);
select * from abcd;
select "id" from abcd;
select id from abcd;
select "ID" from abcd; -- Error: Unknown column 'ID'
select ID from abcd;
drop table ABCD;

create table abcd (ID integer);
select * from abcd;
select "id" from abcd;
select id from abcd;
select "ID" from abcd; -- Error: Unknown column 'ID'
select ID from abcd;
select abcd.ID from abcd;
