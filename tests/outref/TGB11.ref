SELECT * FROM names n1 WHERE n1.id = (SELECT 1 FROM names n2 GROUP BY unknowncolumn,n2.id);
SELECT * FROM names n1 WHERE n1.id = (SELECT 1 FROM names n2 GROUP BY n2.id,n1.id,unknowncolumn);
