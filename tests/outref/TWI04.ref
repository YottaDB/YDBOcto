
-- TWI04 : where IN references parent from SET expression

SELECT *
FROM names a
WHERE a.firstName IN (
    SELECT b.firstName
    FROM names b
    WHERE b.id = a.id

    UNION ALL

    SELECT c.firstName
    FROM names c
    WHERE c.id = a.id
);

