@test "TSC00K : select using a CASE statement" {
  octo <<OCTO 2>&1 | tee output.txt
SELECT
  firstName
  , CASE firstName
    WHEN "Zero" THEN "Zero Cool"
    WHEN "Acid" THEN "Acid Burn"
    WHEN "Cereal" THEN "Cereal Killer"
    ELSE firstName
    END
FROM names;
OCTO
  [[ "$(grep -c "Zero" output.txt)" == "2" ]]
}

